programa {
	funcao inicio() {
        
        inteiro numeros[3][3] = {
            {3,7,2},
            {2,0,4},
            {8,1,5}
        }
        
        para(inteiro i = 0; i< 3; i++){
            para(inteiro c = 0; c < 3; c++){
                escreva(numeros[i][c], " ")
            }
	    
            	escreva("\n")
	    
        }
	
	}
}
