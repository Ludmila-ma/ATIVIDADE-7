programa {
	funcao inicio() {
        
        inteiro soma = 0
        inteiro numeros[3][3] = {
            {3,7,2},
            {2,0,4},
            {8,1,5}
        }
        
        para(inteiro i = 0; i< 3; i++){
            para(inteiro c = 0; c < 3; c++){
                soma = soma + numeros[i][c]
            }
        }
        
        escreva("A soma dos valores Ã©: ", soma)
        
	}
}
